##parser
LEFTANALOG_X=$(
if $(/bin/echo "$contents" | /bin/grep -q "LEFT_ANALOG"); then
	x=$(/bin/echo "$contents" | /bin/grep "LEFT_ANALOG")
	x=$(/bin/echo "$x" | /bin/sed 's/LEFT_ANALOG=//g')
